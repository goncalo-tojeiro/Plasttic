{
  "name": "plasttic",
  "private": true,
  "type": "module",
  "version": "1.4.4",
  "description": "HTML Boilerplate, Atomic Design Sass Framework and Knowledge Hub",
  "keywords": [
    "plasttic",
    "template",
    "boilerplate",
    "front-end"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monkey13-ws/Plasttic.git"
  },
  "author": "Monkey13",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/monkey13-ws/Plasttic/issues"
  },
  "directories": {
    "doc": "docs"
  },
  "files": [
    "docs/CHANGELOG.md",
    "docs/README.md",
    ".vscode/extensions.json",
    ".gitattributes",
    "LICENSE",
    "package.json"
  ],
  "homepage": "https://github.com/monkey13-ws/Plasttic",
  "scripts": {
    "dev": "concurrently npm:copy npm:watch npm:sync",
    "build": "concurrently -n w: npm:copy:* npm:build:* npm:post:build npm:mini:*",
    "build:sass": "sass  --no-source-map src/assets/scss:public/assets/scss",
    "del:public": "del ./public/* ./public/.htaccess",
    "copy": "npm run del:public && concurrently -n w: npm:copy:* npm:post:dev && npm run mini:js ",
    "copy:root": "copyfiles -u 1 -a ./src/* ./src/.htaccess public",
    "copy:private": "copyfiles -u 1 -a ./src/private/**/* public",
    "copy:assets": "copyfiles -u 1 -a ./src/assets/**/* ./src/assets/**/**/* ./src/assets/js/vendor/*.js -e=./src/assets/scss/**/* -e=./src/assets/js/*.js public",
    "watch": "concurrently -n w: npm:watch:*",
    "watch:root": "onchange -d 10000 'src/*' -- npm run copy",
    "watch:assets": "onchange -d 10000 'src/assets/**/*' 'src/assets/**/**/*' 'src/assets/**/**/**/*' -- npm run copy",
    "watch:private": "onchange -d 10000 'src/private/**/*' -- npm run copy",
    "watch:sass": "sass  --no-source-map --watch src/assets/scss/styles.scss:public/assets/scss/styles.css",
    "post:dev": "postcss public/assets/css/*.css public/assets/scss/*.css -u postcss-custom-media postcss-nesting autoprefixer -r --no-map",
    "post:build": "postcss public/assets/css/*.css public/assets/scss/*.css -u postcss-custom-media postcss-nesting autoprefixer cssnano -r --no-map",
    "mini:js": "make-dir public/assets/js/ && uglifyjs 'src/assets/js/*.js' -o 'public/assets/js/scripts.min.js'",
    "mini:html": "rehype public/*.html --use remove-comments --use rehype-minify-whitespace --use rehype-minify-css-style --use rehype-minify-javascript-script --output public",
    "sync": "browser-sync start --port 5500 --no-open --server public --files public",
    "dev:vite": "vite --port 5500"
  },
  "main": "index.html",
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vue/eslint-config-airbnb": "^7.0.0",
    "autoprefixer": "^10.4.13",
    "browser-sync": "^2.27.10",
    "concurrently": "^7.5.0",
    "copyfiles": "^2.4.1",
    "cssnano": "^5.1.14",
    "del-cli": "^5.0.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-wesbos": "^3.1.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-vue": "^9.7.0",
    "make-dir-cli": "^3.0.0",
    "onchange": "^7.1.0",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "postcss-custom-media": "^8.0.2",
    "postcss-import": "^15.0.0",
    "postcss-nesting": "^10.2.0",
    "postcss-preset-env": "^7.8.2",
    "prettier": "^2.7.1",
    "rehype": "^12.0.1",
    "rehype-cli": "^11.0.1",
    "rehype-minify-css-style": "^3.0.0",
    "rehype-minify-javascript-script": "^4.0.0",
    "rehype-minify-whitespace": "^5.0.1",
    "rehype-remove-comments": "^5.0.0",
    "sass": "^1.55.0",
    "svgo": "^3.0.0",
    "typescript": "^4.8.4",
    "uglify-js": "^3.17.4",
    "vite": "^3.2.1",
    "vue-eslint-parser": "^9.1.0"
  }
}
