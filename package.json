{
  "name": "plasttic",
  "type": "module",
  "version": "2.0",
  "description": "HTML Boilerplate, Atomic Design CSS Framework and Best Practices Template",
  "keywords": [
    "plasttic",
    "template",
    "boilerplate",
    "front-end"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tojeiro-me/Plasttic.git"
  },
  "author": "Goncalo Tojeiro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tojeiro-me/Plasttic/issues"
  },
  "directories": {
    "doc": "docs"
  },
  "files": [
    "docs/CHANGELOG.md",
    "docs/README.md",
    ".vscode/extensions.json",
    ".gitattributes",
    "LICENSE",
    "package.json"
  ],
  "homepage": "https://github.com/tojeiro-me/Plasttic",
  "scripts": {
    "dev": "npm run start:dev --dest=dev",
    "build": "npm run start:build  --dest=dist",
    "dev:copy:css": "postcss src/assets/css/styles.css --no-map --dir dev/assets/css/ && postcss src/assets/css/print.css --no-map --dir dev/assets/css/",
    "build:copy:css": "postcss src/assets/css/styles.css --no-map --dir dist/assets/css/ && postcss src/assets/css/print.css --no-map --dir dist/assets/css/",
    "copy:fonts": "copyfiles -u 1 -a ./src/assets/fonts/* $npm_config_dest",
    "copy:html": "copyfiles -u 1 -a ./src/assets/fonts/* $npm_config_dest",
    "copy:img": "copyfiles -u 1 -a ./src/assets/img/**/* $npm_config_dest",
    "copy:js": "copyfiles -u 1 -a ./src/assets/js/vendor/* $npm_config_dest && uglifyjs 'src/assets/js/*.js' -o $npm_config_dest'/assets/js/scripts.min.js'",
    "copy:private": "copyfiles -u 1 -a ./src/private/**/* $npm_config_dest",
    "copy:root": "copyfiles -u 1 -a ./src/* ./src/.htaccess $npm_config_dest",
    "del:css": "del ./$npm_config_dest/assets/css/*  && npm run dev:copy:css",
    "del:fonts": "del ./$npm_config_dest/assets/fonts/* && npm run copy:fonts",
    "del:html": "del ./$npm_config_dest/*.html ./$npm_config_dest/.htaccess && npm run copy:html",
    "del:img": "del ./$npm_config_dest/assets/img/* && npm run copy:img",
    "del:js": "del ./$npm_config_dest/assets/js/* && npm run copy:js",
    "del:private": "del ./$npm_config_dest/private/* ./$npm_config_dest/private/.htaccess && npm run copy:private",
    "del:dest": "del ./$npm_config_dest/* ./$npm_config_dest/**/* ./$npm_config_dest/.htaccess",
    "del:root": "del ./$npm_config_dest/*.* ./$npm_config_dest/.htaccess && npm run copy:root",
    "mini:html": "rehype --setting allowDangerousCharacters:true $npm_config_dest/*.html  --use format --use remove-comments --use minify-css-style --use minify-javascript-script --output $npm_config_dest",
    "start:build": "npm run del:dest && concurrently npm:copy:* npm:build:copy:css npm:mini:*",
    "start:dev": "npm run del:dest && concurrently npm:copy:* npm:dev:copy:css npm:mini:html && concurrently npm:watch:* npm:sync",
    "sync": "browser-sync start --port 8000 --no-open --server $npm_config_dest --files $npm_config_dest",
    "watch:css": "onchange -d 10000 'src/assets/css/*' -- npm run del:css",
    "watch:fonts": "onchange -d 10000 'src/assets/fonts/*' -- npm run del:fonts",
    "watch:html": "onchange -d 10000 'src/*.html' -- npm run del:root",
    "watch:img": "onchange -d 10000 'src/assets/img/*' 'src/assets/img/**/*' 'src/assets/img/**/**/*' -- npm run del:img",
    "watch:js": "onchange -d 10000 'src/assets/js/**/*' -- npm run del:js",
    "watch:private": "onchange -d 10000 'src/private/**/*' -- npm run del:private",
    "watch:root": "onchange -d 10000 'src/*.*' -- npm run del:root"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vue/eslint-config-airbnb": "^7.0.0",
    "autoprefixer": "^10.4.13",
    "browser-sync": "^2.27.11",
    "concurrently": "^7.6.0",
    "copyfiles": "^2.4.1",
    "cssnano": "^5.1.14",
    "del-cli": "^5.0.0",
    "engine.io": "^6.3.1",
    "eslint": "^8.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-wesbos": "^3.2.3",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-vue": "^9.9.0",
    "imageoptim-cli": "^3.0.7",
    "lighthouse": "^9.6.8",
    "make-dir-cli": "^3.0.0",
    "onchange": "^7.1.0",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-custom-media": "^9.0.1",
    "postcss-discard-empty": "^5.1.1",
    "postcss-import": "^15.1.0",
    "postcss-media-minmax": "^5.0.0",
    "postcss-mixins": "^9.0.4",
    "postcss-nested": "^6.0.0",
    "postcss-preset-env": "^7.8.3",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.3",
    "puppeteer": "^19.5.2",
    "rehype": "^12.0.1",
    "rehype-cli": "^11.0.1",
    "rehype-format": "^4.0.1",
    "rehype-minify-css-style": "^3.0.0",
    "rehype-minify-javascript-script": "^4.0.0",
    "rehype-minify-whitespace": "^5.0.1",
    "rehype-remove-comments": "^5.0.0",
    "sass": "^1.57.1",
    "socket.io": "^4.5.4",
    "svgexport": "^0.4.2",
    "svgo": "^3.0.2",
    "typescript": "^4.9.4",
    "uglify-js": "^3.17.4",
    "vue-eslint-parser": "^9.1.0"
  }
}
